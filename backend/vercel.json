{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/random",
      "destination": "/api/random"
    },
    {
      "source": "/search",
      "destination": "/api/search"
    },
    {
      "source": "/mal/:slug",
      "destination": "/api/mal?slug=:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/api/404"
    }
  ]
}
